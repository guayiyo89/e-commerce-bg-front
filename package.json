    "@angular/router": "~12.2.0",
    "@types/jest": "^29.4.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "jest-preset-angular": "^13.0.0",
    "rxjs": "~6.6.0",
